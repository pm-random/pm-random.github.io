{
  "name": "pm-random",
  "version": "4.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --mode dev",
    "prod": "vite",
    "dev.host": "vite --mode dev --host --port 8080",
    "prod.host": "vite --host --port 8080",
    "preview": "vite preview --host --port 8080",
    "build": "run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "clean": "npm run format && npm run lint",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --max-warnings=0",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "highcharts": "^11.4.4",
    "highcharts-vue": "^1.4.3",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.9",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.27.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.3.2",
    "typescript": "~5.3.3",
    "vite": "^5.3.2",
    "vue-tsc": "^1.8.27"
  }
}
